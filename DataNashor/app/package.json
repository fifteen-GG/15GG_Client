    "@types/styled-components": "^5.1.26",
    "axios": "^1.1.2",
    "electron-is-dev": "^2.0.0",
    "lottie-react": "^2.3.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
